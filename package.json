{
    "name": "rc-tracks-editor",
    "version": "0.0.0",
    "description": "An editor to build a RC track using the tiles from jsconan/rc-tracks/scale-64",
    "license": "GPL-3.0",
    "author": "Jean-SÃ©bastien CONAN",
    "homepage": "https://github.com/jsconan/rc-tracks-editor#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jsconan/rc-tracks-editor.git"
    },
    "bugs": {
        "url": "https://github.com/jsconan/rc-tracks-editor/issues"
    },
    "keywords": [
        "rc",
        "track",
        "editor"
    ],
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "jest --config=./jest.config.cjs",
        "test:watch": "npm test -- --watch",
        "test:update": "npm test -- --updateSnapshot",
        "test:cov": "npm test -- --coverage",
        "coverage:html": "open-cli .coverage/lcov-report/index.html",
        "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore --ignore-path .eslintignore .",
        "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
        "spell": "cspell lint 'src/**/*.{svelte,js,md}'"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.19.4",
        "@sveltejs/vite-plugin-svelte": "^1.0.9",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/svelte": "^3.2.2",
        "@testing-library/user-event": "^14.4.3",
        "@vitejs/plugin-legacy": "^2.2.0",
        "babel-jest": "^29.2.0",
        "cspell": "^6.12.0",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-es": "^4.1.0",
        "eslint-plugin-jest": "^27.1.2",
        "eslint-plugin-svelte3": "^4.0.0",
        "jest": "^29.2.0",
        "jest-environment-jsdom": "^29.2.0",
        "jest-transform-stub": "^2.0.0",
        "open-cli": "^7.1.0",
        "prettier": "^2.7.1",
        "prettier-plugin-svelte": "^2.8.0",
        "svelte": "^3.52.0",
        "svelte-jester": "^2.3.2",
        "svelte-preprocess": "^4.10.7",
        "vite": "^3.0.8"
    }
}
